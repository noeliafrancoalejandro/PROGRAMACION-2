#include "stdafx.h"
#include<iostream>
#include"conio.h"
using namespace std;

void main()
{
 int suma,i,N;
 cout<< "ingresarel valor de N:";
 cin>> N;
 suma=0;
  for(i=1;i<=N;i++)
	  suma=suma+i;
  cout<<"la suma de los "<<"primeros numeros naturaleses:"<<suma;
  getch();
 	
}
