#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;

void main ()
{
float h,m,s,H,M,S,t,r,p;
cout<<"ingrese el valor de las horas de a";
cin>>h;
cout<<"ingrese el valor de minutos de a";
cin>>m;
cout<<"ingrese el valor de segundos de a";
cin>>s;
cout<<"ingrese el valor de las horas de b";
cin>>H;
cout<<"ingrese el valor de minutos de b";
cin>>M;
cout<<"ingrese el valor de segundos de b";
cin>>S;
if((h>60)&&(H>60))
   t=h-H;
cout<<t;
if((m>60)&&(M>60))
   r=m-M;
cout<<r;
if((s>60)&&(S))
  p=s-S;
cout<< p;
getch();
}
